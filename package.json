{
  "name": "i18n-label-merger",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "bin": "npm/i18n-label-merger",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:windows": "cargo build --release --target=x86_64-pc-windows-gnu",
    "build:linux": "cargo build --release --target=x86_64-unknown-linux-gnu",
    "build:mac": "cargo build --release --target=x86_64-apple-darwin",
    "build:mac-arm64": "cargo build --release --target=aarch64-apple-darwin",
    "build:wasm": "wasm-pack build crates/wasm --target nodejs",
    "build:all": "npm run build:windows && npm run build:linux && npm run build:mac && npm run build:mac-arm64 && npm run build:wasm",
    "postinstall": "node ./npm/postinstall.js",
    "debug:wasm": "node npm/debug-wasm.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "files": [
    "npm/*"
  ],
  "dependencies": {
    "binary-install": "^1.1.0"
  },
  "devDependencies": {
    "wasm-pack": "^0.12.1"
  }
}
